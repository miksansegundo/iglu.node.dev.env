{
  "name": "iglu.node.dev.env",
  "version": "0.0.0",
  "description": "Node.js Development environment for Express.js apps at Iglu.net",
  "main": "index.js",
  "scripts": {
    "prepublish": "echo \"Compile with babel!\" || true",
    "code.coverage": "nyc --reporter=lcov --reporter=text --reporter=html --require babel-register --extension .js npm test",
    "open.coverage": "open coverage/index.html",
    "test": "mocha --require test/helpers/setup.js",
    "watch.service": "NODE_ENV=development node_modules/.bin/nodemon -w src index.js",
    "run.service": "dumb-init node index.js",
    "start": "sh scripts/run.service.sh",
    "lock": "npm shrinkwrap --dev",
    "lint": "standard --fix && standard && echo 'Good Standad code :)' || echo 'Not good Standad dode :('"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miksansegundo/iglu.node.dev.env.git"
  },
  "author": "miguelsansegundo@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miksansegundo/iglu.node.dev.env/issues"
  },
  "homepage": "https://github.com/miksansegundo/iglu.node.dev.env#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "body-parser": "^1.15.2",
    "express": "^4.13.3",
    "falcor-express": "^0.1.2",
    "ramda": "^0.22.1"
  },
  "devDependencies": {
    "livereload": "^0.6.0",
    "nodemon": "^1.11.0",
    "mocha": "^3.1.2",
    "nyc": "^8.3.2",
    "standard": "^8.4.0"
  }
}
